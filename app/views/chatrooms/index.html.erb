<h1>Chats page</h1>
<h2>Hello <%= current_user.username %></h2>
<% current_user.chats.each do |chat| %>
<%= link_to chat_path(chat)  do %>
  <%= chat.users.where.not(username: current_user.username).map {|user| user.username}.join(", ") %>

<% end %>
<br>
<% end %>
<br> <br>

<%= link_to "Log out", logout_path, method: "destroy" %>